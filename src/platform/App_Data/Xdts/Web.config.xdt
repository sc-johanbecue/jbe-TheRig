<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
    <location path="sitecore">
        <system.webServer>
            <httpProtocol>
                <customHeaders>
                    <add xdt:Transform="SetAttributes" xdt:Locator="Match(name)" name="Content-Security-Policy" value="img-src 'self' https://lpoc-p-001.sitecorecontenthub.cloud; child-src 'self' https://lpoc-p-001.sitecorecontenthub.cloud; connect-src 'self' https://lpoc-p-001.sitecorecontenthub.cloud;" />
                </customHeaders>
            </httpProtocol>
        </system.webServer>
    </location>
</configuration>